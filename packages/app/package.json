{
  "name": "react-template",
  "version": "0.0.0",
  "description": "React template project.",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf .turbo dist node_modules src/*.gen.ts",
    "check:format": "prettier --check . --ignore-path ../../.gitignore",
    "check:lint": "eslint",
    "fix:format": "prettier --write . --ignore-path ../../.gitignore",
    "fix:lint": "eslint --fix",
    "start": "vite",
    "test": "vitest --run"
  },
  "author": "Kirin Patel <contact@kirinpatel.com>",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "config-eslint": "*",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
